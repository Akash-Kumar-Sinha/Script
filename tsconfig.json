{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "paths": {
      "cmdk": ["./node_modules/cmdk/dist/index.d.ts"],
      "@radix-ui/react-context-menu": ["./node_modules/@radix-ui/react-context-menu/dist/index.d.ts"],
      "@radix-ui/react-aspect-ratio": ["./node_modules/@radix-ui/react-context-menu/dist/index.d.ts"],
      "react-resizable-panels": ["./node_modules/@radix-ui/react-context-menu/dist/index.d.ts"],
      
    },
  },
  "include": ["src/**/*.ts", "src/**/*.tsx"]
}
